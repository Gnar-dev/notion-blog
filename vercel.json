{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "functions": {
    "app/blog/[slug]/page.tsx": {
      "maxDuration": 30
    }
  },
  "env": {
    "NOTION_TOKEN": "@notion_token",
    "NOTION_DATABASE_ID": "@notion_database_id"
  }
}
